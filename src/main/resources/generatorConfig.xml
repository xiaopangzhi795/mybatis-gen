<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
<!--    DOC: https://mybatis.org/generator/index.html-->

<!--    targetRuntime=MyBatis3DynamicSql, MyBatis3, MyBatis3Simple-->
    <context id="mysql" targetRuntime="MyBatis3DynamicSql" defaultModelType="flat">
        <property name="autoDelimitKeywords" value="ture"/>
        <property name="beginningDelimiter" value="`"/>
        <property name="endingDelimiter" value="`"/>
        <property name="javaFileEncoding" value="UTF-8"/>

        <!-- 为模型生成序列化方法-->
        <plugin type="org.mybatis.generator.plugins.SerializablePlugin"/>
        <!-- 为生成的Java模型创建一个toString方法 -->
        <plugin type="org.mybatis.generator.plugins.ToStringPlugin"/>

<!--        <plugin type="org.mybatis.generator.plugins.SerializablePlugin"/>-->

<!--        <commentGenerator>-->
<!--            <property name="suppressAllComments" value="true"/>-->
<!--        </commentGenerator>-->

<!--        JDBC 链接配置-->
        <commentGenerator>
            <!-- 是否去除自动生成的注释 true：是 ：false:否 -->
            <property name="suppressAllComments" value="false"/>
            <property name="addRemarkComments" value="true"/>
            <property name="suppressDate" value="true"/>
        </commentGenerator>

        <jdbcConnection driverClass="com.mysql.cj.jdbc.Driver"
                        connectionURL="jdbc:mysql://rm-2zez570bgf6ylhpd6uo.mysql.rds.aliyuncs.com/geek-config?useUnicode=true&amp;characterEncoding=utf-8&amp;useSSL=true&amp;serverTimezone=UTC"
                        userId="rubik"
                        password="YTDXCMQ5b7sphr">
            <property name="nullCatalogMeansCurrent" value="true" />
        </jdbcConnection>

        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>

        <javaModelGenerator targetPackage="com.geek45.mybatisdemo.dal.model"
                            targetProject="src/main/java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="false"/>
        </javaModelGenerator>

        <sqlMapGenerator targetPackage="com.geek45.mybatisdemo.dal"
                        targetProject="src/main/java">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>

        <javaClientGenerator type="XMLMAPPER"
                             targetProject="src/main/java"
                             targetPackage="com.geek45.mybatisdemo.dal">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>

        <!--生成全部表tableName设为%-->
<!--        <table tableName="geek_config" domainObjectName="geekConfigDO">-->
<!--            <generatedKey column="id" sqlStatement="JDBC"/>-->
<!--        </table>-->

        <table tableName="%">
            <generatedKey column="id" sqlStatement="JDBC"/>
        </table>


    </context>


</generatorConfiguration>